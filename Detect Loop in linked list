bool detectLoop(Node* head)
{
    if(head == NULL)
    {
        return false;
    }
    Node* slow = head;
    Node* fast = head;
    while(fast != NULL && fast -> next != NULL)
    {
        fast = fast -> next -> next;
        slow = slow -> next;

        if(fast == slow)
        {
            return true;
        }
    }
    return false;
}
